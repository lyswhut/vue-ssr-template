{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "description": "A vue-spa based scaffolding.",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production node build-config/build-prod.js",
    "build:test:prod": "cross-env NODE_ENV=production RUN_MODE=test node build-config/build-prod.js",
    "build": "npm run clean && npm run build:prod",
    "buildt": "npm run clean && npm run build:test:prod",
    "btr": "npm run buildt && npm run devt",
    "clean": "rimraf public",
    "dev": "nodemon server/server.js",
    "devt": "cross-env NODE_ENV=production RUN_MODE=test node server/server.js",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "lint": "eslint -f ./node_modules/eslint-formatter-friendly --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint -f ./node_modules/eslint-formatter-friendly --fix --ext .js --ext .jsx --ext .vue client/"
  },
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/vue-spa-template.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/vue-spa-template/issues"
  },
  "homepage": "https://github.com/lyswhut/vue-spa-template#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.1",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "koa-webpack": "^5.2.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static-cache": "^5.1.2",
    "log4js": "^4.3.1",
    "lru-cache": "^5.1.1",
    "memory-fs": "^0.4.1",
    "vue": "^2.6.10",
    "vue-meta": "^2.0.3",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  }
}
