{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "description": "A vue-spa based scaffolding.",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production node build-config/build-prod.js",
    "build:test:prod": "cross-env NODE_ENV=production RUN_MODE=test node build-config/build-prod.js",
    "build": "npm run clean && npm run build:prod",
    "buildt": "npm run clean && npm run build:test:prod",
    "btr": "npm run buildt && npm run devt",
    "clean": "rimraf public",
    "dev": "nodemon server/server.js",
    "devt": "cross-env NODE_ENV=production RUN_MODE=test node server/server.js",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "lint": "eslint -f ./node_modules/eslint-formatter-friendly --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint -f ./node_modules/eslint-formatter-friendly --fix --ext .js --ext .jsx --ext .vue client/"
  },
  "browserslist": [
    ">= 1%",
    "last 1 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/vue-spa-template.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/vue-spa-template/issues"
  },
  "homepage": "https://github.com/lyswhut/vue-spa-template#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.14.5",
    "chokidar": "^3.4.3",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "file-loader": "^6.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "koa-webpack": "^5.3.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.11.3",
    "nodemon": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^9.4.0",
    "koa-send": "^5.0.1",
    "koa-static-cache": "^5.1.4",
    "log4js": "^6.3.0",
    "lru-cache": "^6.0.0",
    "memory-fs": "^0.5.0",
    "vue": "^2.6.12",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.7",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  }
}
